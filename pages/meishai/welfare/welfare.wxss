.weifare-container{
  width:100%;
  height:100%;
  background: rgb(245, 245, 245);
}
.weifare-scroll{
  width:100%;
  height: 2000px;
}
.weifare-scroll-top{
  width: 100%;
  height: 196px;
}
.weifare-scroll-top li{
  float: left;
  box-sizing: border-box;
  width: 50%;
  height: 95px;
  padding: 1.5px;
}
.weifare-scroll-top li image{
  width: 100%;
  height: 100%;
}

.weifare-scroll-bottom{
  width: 100%;
  margin-top: 8px;
}
.weifare-scroll-dl{
  width: 100%;
  height: 235px;
  margin-bottom: 10px;
}
.weifare-scroll-dl dt{
  width: 100%;
  height: 160px;
}
.weifare-scroll-dl dt image{
  width: 100%;
  height: 160px;
}
.weifare-scroll-dl dd{
  width: 100%;
  height: 75px;
  box-sizing: border-box;
  padding-left: 15px;
}
.weifare-scroll-dl dd span{
  width: 100%;
  height: 36px;
  line-height: 36px;
  font-size: 10px;
  color: rgb(106, 106, 106);
}
.weifare-scroll-dl dd .weifare-productInfo{
  width: 100%;
  height: 39px;
  line-height: 40px;
  color: rgb(176,176,176);
  font-size: 8px;
}
.weifare-scroll-dl dd .weifare-productInfo span{
  float: left;
  height: 100%;
}
.weifare-scroll-dl dd .weifare-productInfo .productinfo1{
  width: 100px;
}
.weifare-scroll-dl dd .weifare-productInfo .productinfo2{
  width: 80px;
}
.weifare-scroll-dl dd .weifare-productInfo .productinfo3{
  float: right;
  width: 30px;
}
